package hitchbot.main;

import net.minecraft.client.Minecraft;

public class Module {
	
	protected Minecraft mc = Minecraft.getMinecraft();
	private int key;
	private String name;
	private boolean toggled;
	private Category category;
	
	public Module(String nm, int k, Category c) {
		name = nm;
		key = k;
		category = c;
		toggled = false;
	}
}
